{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to strapi",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch current project with ts-node",
            "protocol": "inspector",
            "cwd": "${workspaceRoot}",
            "args": [
                "${workspaceFolder}/src/infoSender.ts",
                "--eventId",
                "test",
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register",
                "--harmony"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "smartStep": true
        }
    ]
}